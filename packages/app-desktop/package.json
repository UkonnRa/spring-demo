{
  "name": "white-rabbit-app-desktop",
  "version": "0.1.0",
  "packageManager": "yarn@4.0.2",
  "main": ".vite/build/main.js",
  "author": "Ukonn Ra <ukonnra@outlook.com>",
  "description": "Wonderland WhiteRabbit Desktop",
  "license": "MIT",
  "scripts": {
    "dev": "electron-forge start",
    "build:debug": "electron-forge package",
    "build": "yarn clean && electron-forge make",
    "test": "vitest run --coverage",
    "lint": "yarn lint:eslint && yarn lint:tsc",
    "lint:tsc": "vue-tsc --noEmit",
    "lint:eslint": "eslint '**/*.{vue,ts,js,mjs}' --ignore-path ../../.eslintignore",
    "lint:fix": "yarn lint:eslint --fix",
    "clean": "rimraf .vite/ node_modules/ out/",
    "gen-proto": "grpc_tools_node_protoc --js_out=import_style=commonjs,binary:../helloworld/static_codegen/ --grpc_out=grpc_js:../helloworld/static_codegen/ helloworld.proto"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.9.13",
    "white-rabbit-frontend-core": "workspace:^"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.2.0",
    "@electron-forge/maker-deb": "^7.2.0",
    "@electron-forge/maker-rpm": "^7.2.0",
    "@electron-forge/maker-squirrel": "^7.2.0",
    "@electron-forge/maker-zip": "^7.2.0",
    "@electron-forge/plugin-vite": "^7.2.0",
    "electron": "28.1.3",
    "rimraf": "^5.0.5",
    "vite": "^5.0.11",
    "vue-tsc": "^1.8.27"
  }
}
