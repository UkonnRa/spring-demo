{
  "packageManager": "yarn@4.0.2",
  "version": "0.1.0",
  "engines": {
    "yarn": ">=4",
    "node": ">=20"
  },
  "private": true,
  "devDependencies": {
    "@commitlint/cli": "^18.4.4",
    "@commitlint/config-conventional": "^18.4.4",
    "commitizen": "^4.3.0",
    "commitlint": "^18.4.4",
